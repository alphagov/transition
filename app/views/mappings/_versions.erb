<table class="table table-striped">
  <thead>
  <tr>
    <th>By</th>
    <th>On</th>
    <th>Changes</th>
  </tr>
  </thead>
  <tbody>
  <% @mapping.versions.reverse.each do |version| %>
      <tr>
        <td>
          <%= anchor '[*]', version.id %>
          <%= version.whodunnit %>
        </td>
        <td>
          <%= version.created_at.strftime('%d/%m/%y %H:%M') %>
        </td>
        <td>
          <%= render partial: 'versions/changeset_table', locals: { version: version } %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
